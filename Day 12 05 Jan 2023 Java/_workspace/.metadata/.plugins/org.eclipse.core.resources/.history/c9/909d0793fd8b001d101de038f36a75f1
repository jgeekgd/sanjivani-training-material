package stringdemo;

public class StringApiDemo {

	public static void main(String[] args) {
		String sms="Hi";
		String sms1="Hi";
		System.out.println(sms.hashCode());
		System.out.println(sms1.hashCode());
		
		String sms3=new String("Hi");
		System.out.println(sms.hashCode());
		System.out.println(sms3.hashCode());
	}

}
