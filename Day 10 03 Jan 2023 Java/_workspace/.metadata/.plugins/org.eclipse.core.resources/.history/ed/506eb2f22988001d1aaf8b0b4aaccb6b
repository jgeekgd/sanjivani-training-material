package com.revature.features;

public class SwitchCase {
	public static void main(String[] args) {

		String roomType="LUXURY";
		switch(roomType) {
		case "AC": System.out.println("AC");
		case "DEULUX": System.out.println("Sofa");
		case "SUPER DEULUX": System.out.println("Study Table");
		case "SUITE": System.out.println("Kitechn");
		case "LUXURY": System.out.println("Infity Pool");
		
		}
		
	}
}
