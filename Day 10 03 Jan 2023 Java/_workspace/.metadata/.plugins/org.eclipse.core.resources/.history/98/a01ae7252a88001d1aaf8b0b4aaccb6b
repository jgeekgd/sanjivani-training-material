package com.revature.features;

public class SwitchCase {
	public static void main(String[] args) {

		String roomType = "DEULUX";
		switch (roomType) {
		case "LUXURY":
			System.out.println("Infity Pool");
		case "SUITE":
			System.out.println("Kitechn");
		case "SUPER DEULUX":
			System.out.println("Study Table");
		case "DEULUX":
			System.out.println("Sofa");
		case "AC":
			System.out.println("AC");
		}

	}
}
